<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="address">
        <container name="customer_default_billing_address_wrapper">
            <component name="customer_default_billing_address_content" template="MobWeb_SwissAddressFormat/default-address" />
        </container>
        <container name="customer_default_shipping_address_wrapper">
            <component name="customer_default_shipping_address_content" template="MobWeb_SwissAddressFormat/default-address" />
        </container>
    </fieldset>
</form>
